{
  "expo": {
    "name": "Santé Diabète",
    "slug": "sante-diabet-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-logo.png",
    "scheme": "santediabetmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "backgroundColor": "#FF0000",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "native",
      "backgroundColor": "#FF0000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon.png",
        "backgroundColor": "#FF0000"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/splash-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "cover",
          "backgroundColor": "#FF0000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
